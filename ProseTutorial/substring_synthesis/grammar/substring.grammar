using semantics SubstringSynthesis.Semantics;
using learners SubstringSynthesis.WitnessFunctions;

language Substring;

@complete feature double Score = SubstringSynthesis.RankingScore;

@input string s;

@start string program := cat;

string cat := substring

string substring := SubstringPP(s, pos, pos) | SubstringPL(s, pos, l)

int pos := AbsPos(s, k) | RelPosLeft(s, r) | RelPosRight(s, r)

int k;
int l;
Regex r;